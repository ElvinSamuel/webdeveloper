<!--Including The Header And Footer Files (And CSS By Extension)-->
<% include partials/header %> 

<h2>Some Friends (Don't Worry About Where I Got The Names...</h2>

<% friends.forEach(function(friend){ %>
    <li>I have a friend named: <%=friend %></li>
<% }); %>

<form action="/addFriend" method="POST">
    <input type="text" name="newfriend" placeholder="Add A New Friend's Name Here!">
    <button>Add New Friend!</button>
</form>

<% include partials/footer %>